<?xml version="1.0" encoding="utf-8"?>
<xs:schema
  xmlns:bdo="http://meltingsoft.com/bindopen/xsd"
  elementFormDefault="qualified" targetNamespace="http://meltingsoft.com/bindopen/xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:simpleType name="RequirementLevel">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="Forbidden" />
      <xs:enumeration value="Optional" />
      <xs:enumeration value="OptionalExclusively" />
      <xs:enumeration value="Required" />
    </xs:restriction>
  </xs:simpleType>
  
</xs:schema>