<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>BindOpen.Tests.UnitTest</AssemblyName>
    <Description>BindOpen is a framework that allows you to build widely-extended applications. This package conducts unit tests.</Description>
    <Authors>Sumer Team</Authors>
    <Company>BindOpen</Company>
    <Product>BindOpen.Framework</Product>
    <Copyright>Copyright ©BindOpen 2011-2020</Copyright>
    <PackageId>BindOpen.Tests.UnitTest</PackageId>
    <RootNamespace>BindOpen.Tests.UnitTest</RootNamespace>
    <PackageProjectUrl>https://github.com/bindopen</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/bindopen/BindOpen/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/bindopen/BindOpen.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageIconUrl>https://bindopen.blob.core.windows.net/resources/pictures/logos/bdo_icon_128x128.png</PackageIconUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>bin\</OutputPath>
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="bdo\logs\**" />
