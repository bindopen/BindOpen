    <None Remove="bdo\logs\**" />
    <None Remove="Data\References\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="System\Diagnostics\ApiLogDtoTest.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.4.0" />
    <PackageReference Include="NUnit" Version="3.12.0" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.15.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\BindOpen.Databases.MSSqlServer\BindOpen.Databases.MSSqlServer.csproj" />
    <ProjectReference Include="..\..\src\BindOpen.Runtime.NetCore\BindOpen.Runtime.NetCore.csproj" />
  </ItemGroup>

</Project>
