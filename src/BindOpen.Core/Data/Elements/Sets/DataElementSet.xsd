<?xml version="1.0" encoding="utf-8"?>
<xs:schema
  xmlns:bdo="https://bindopen.org/xsd"
  elementFormDefault="qualified" targetNamespace="https://bindopen.org/xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:include schemaLocation="StoredDataItem.xsd" />
  <xs:include schemaLocation="CarrierElement.xsd" />
  <!--<xs:include schemaLocation="DocumentElement.xsd" />-->
  <xs:include schemaLocation="EntityElement.xsd" />
  <xs:include schemaLocation="MetaDataElement.xsd" />
  <xs:include schemaLocation="ObjectElement.xsd" />
  <xs:include schemaLocation="ScalarElement.xsd" />
  <xs:include schemaLocation="SchemaElement.xsd" />
  <xs:include schemaLocation="SourceElement.xsd" />

  <xs:complexType name="DataElementSet">
    <xs:complexContent mixed="false">
      <xs:extension base="bdo:DataItem">
        <xs:sequence>
          <xs:choice maxOccurs="unbounded">
            <xs:element minOccurs="0" maxOccurs="unbounded" name="carrier" nillable="true" type="bdo:CarrierElement" />
            <!--<xs:element minOccurs="0" maxOccurs="unbounded" name="documentElement" nillable="true" type="bdo:DocumentElement" />-->
            <xs:element minOccurs="0" maxOccurs="unbounded" name="entity" nillable="true" type="bdo:EntityElement" />
            <xs:element minOccurs="0" maxOccurs="unbounded" name="object" nillable="true" type="bdo:ObjectElement" />
            <xs:element minOccurs="0" maxOccurs="unbounded" name="meta" nillable="true" type="bdo:MetaDataElement" />
            <xs:element minOccurs="0" maxOccurs="unbounded" name="scalar" nillable="true" type="bdo:ScalarElement" />
            <xs:element minOccurs="0" maxOccurs="unbounded" name="schema" nillable="true" type="bdo:SchemaElement" />
            <xs:element minOccurs="0" maxOccurs="unbounded" name="source" nillable="true" type="bdo:SourceElement" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
</xs:schema>